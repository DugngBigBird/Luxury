-- Scripts:

local function NYDEA_fake_script() -- ScreenGui.NOTIFIACOTn] 
	local script = Instance.new('LocalScript', ScreenGui)

	wait(0.1)
	
	game.StarterGui:SetCore("SendNotification", {
		Title = "Luxury Whitelist";
		Text = "Load Successfully";
		Duration = "6";
	})
end
coroutine.wrap(NYDEA_fake_script)()
local function WDIGG_fake_script() -- Frame.LoadScript 
	local script = Instance.new('LocalScript', Frame)

	local Frame = script.Parent.Parent.Frame
	local Menu = script.Parent.Parent.Menu
	local LogoLuxury = script.Parent.Parent.LogoLuxury
	local Welcome1 = script.Parent.Parent.Welcome1
	local Welcome2 = script.Parent.Parent.Welcome2
	local ColorOpenMenu = script.Parent.Parent.ColorOpenMenu
	local LuxuryOpenMenu = script.Parent.Parent.LuxuryOpenMenu
	local UploadLuxury = script.Parent.Parent.UploadLuxury
	
	Menu.Visible = false
	LogoLuxury.Visible = false
	Welcome1.Visible = false
	Welcome2.Visible = false
	ColorOpenMenu.Visible = false
	LuxuryOpenMenu.Visible = false
	UploadLuxury.Visible = false
	
	if Frame.Visible == false then
		wait(6.7)
		game.Lighting.Blur.Enabled = true
		Menu.Visible = true
		
		wait(1.4)
		Welcome1.Visible = true
		Welcome2.Visible = true
		
		wait(0.1)
		LogoLuxury.Visible = true
		
		wait(1.6)
		ColorOpenMenu.Visible = true
		LuxuryOpenMenu.Visible = true
		UploadLuxury.Visible = true
		
		wait(11.1)
		game.Lighting.Blur.Enabled = false
		ColorOpenMenu.Visible = true
		LuxuryOpenMenu.Visible = true
		UploadLuxury.Visible = false
		LogoLuxury.Visible = true
		Welcome1.Visible = false
		Welcome2.Visible = false
		Menu.Visible = false
		
		wait(0.1)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/markxd07/Luxury-Free/main/Auto_Kaitun.lua"))()
		end
	end
end
coroutine.wrap(WDIGG_fake_script)()
local function WPXD_fake_script() -- UploadLuxury.LocalScript 
	local script = Instance.new('LocalScript', UploadLuxury)

	while wait () do
		script.Parent.Rotation = script.Parent.Rotation + 6
	end
end
coroutine.wrap(WPXD_fake_script)()
